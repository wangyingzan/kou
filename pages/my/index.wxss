page{
    background: #F8FAF9;
}

.header{
    position: relative;
    /*height: 234rpx;*/
    /*padding-bottom: 10rpx;*/
}
.header .navTitle{
    position: relative;
    z-index: 10;
}
.header .navTitle .title{
    font-size: 36rpx;
    font-weight: bold;
    text-align: center;
    color: #222222;
}
.header .headerBg{
    position: absolute;
    top: 0;
    width: 100%;
    height: 406rpx;
}
.header .login{
    height: 234rpx;
    display: flex;
    align-items: center;
    padding: 0 40rpx;
    padding-bottom: 40rpx;
    color: #222222;
}
.header .login .avatar{
    width: 124rpx;
    height: 124rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}
.header .login .log{
    display: flex;
    align-items: center;
    position: relative;
    font-size: 36rpx;
    font-weight: bold;
    color: #333;
    z-index: 100;
}
.header .login .log .arrow{
    width: 12rpx;
    height: 22rpx;
    margin-left: 10rpx;
}
.container{
    padding: 0 24rpx;
}
.mine{
    padding: 0 25rpx;
    border-radius: 12rpx;
    background: #fff;
    margin-bottom: 32rpx;
}
.mine .mineHeader{
    position: relative;
    display: flex;
    justify-content: space-between;
    height: 86rpx;
    font-size: 28rpx;
    font-weight: bold;
    color: #222222;
    line-height: 86rpx;
    border-bottom: 2rpx solid #eee;

}
.mine .mineHeader .right{
    display: flex;
    align-items: center;
    font-weight: normal;
    font-size: 24rpx;
    color: #666666;
}
.mine .mineHeader .right .arrow{
    width: 8rpx;
    height: 16rpx;
    margin-left: 10rpx;
}
.mine .tabs{
    height: 168rpx;
    padding: 0 25rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.mine .tabs .item{
    position: relative;
    font-size: 28rpx;
    text-align: center;
    color: #222222;
}
.mine .tabs .num{
    position: absolute;
    top: -16rpx;
    right: -5rpx;
    width: 32rpx;
    height: 28rpx;
    font-weight: bold;
    text-align: center;
    color: #fff;
    font-size: 20rpx;
    border-radius: 64rpx;
    background: #F73A3A;
}
.mine .tabs .item .icon{
    width: 56rpx;
    height: 56rpx;
    margin-bottom: 8rpx;
    display: inline-block;
}

.other{
    display: flex;
    flex-wrap: wrap;
    gap:36rpx 31rpx;
    padding: 40rpx 24rpx ;
    border-radius: 12rpx;
    background: #fff;
}
.other .item{
    padding: 0;
    width: 140rpx;
    line-height: 28rpx;
    font-size: 28rpx;
    text-align: center;
    color: #222222;
    background: #fff;
}
.other button{
    margin: 0;
}
.other .item:after{
    border: none;
}
.other .item .icon{
    display: inline-block;
    width: 56rpx;
    height: 56rpx;
    margin-bottom: 16rpx;
}


.myBottom{
    padding-top: 140rpx;
    text-align: center;
    font-size: 28rpx;
    color: #CCCCCC;
}
.myBottom .title{
    font-weight: bold;
    margin-bottom: 16rpx;
}
