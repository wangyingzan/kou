page{
    background: #F8FAF9;
}

.header{
    position: relative;
    height: 495rpx;
    margin-bottom: -86rpx;
}
.header .navTitle{
    position: relative;
    z-index: 10;
}
.header .navTitle .title{
    font-size: 36rpx;
    text-align: center;
    color: #222222;
}
.header .headerBg{
    position: absolute;
    top: 0;
    width: 100%;
    height: 406rpx;
}
.header .login{
    height: 234rpx;
    display: flex;
    align-items: center;
    padding: 0 40rpx;

    color: #222222;
}
.header .login .avatar{
    width: 124rpx;
    height: 124rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}
.header .login .log{
    position: relative;
    font-size: 36rpx;
    font-weight: bold;
    color: #333;
    z-index: 100;
}

.container{
    padding: 0 24rpx;
}
.mine{
    padding: 0 25rpx;
    border-radius: 12rpx;
    background: #fff;
    margin-top: -85rpx;
    margin-bottom: 32rpx;
}
.mine .mineHeader{
    display: flex;
    justify-content: space-between;
    height: 86rpx;
    font-size: 28rpx;
    font-weight: bold;
    color: #222222;
    line-height: 86rpx;
    border-bottom: 2rpx solid #eee;

}
.mine .mineHeader .right{
    font-weight: normal;
    font-size: 24rpx;
    color: #666666;
}
.mine .tabs{
    height: 168rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.mine .tabs .item{
    font-size: 28rpx;
    text-align: center;
    color: #222222;
}
.mine .tabs .item .icon{
    width: 56rpx;
    height: 56rpx;
    margin-bottom: 8rpx;
    display: inline-block;
}

.other{
    display: flex;
    flex-wrap: wrap;
    gap:30rpx 18rpx;
    padding: 24rpx 40rpx;
    border-radius: 12rpx;
    background: #fff;
}
.other .item{
    width: 140rpx;
    font-size: 28rpx;
    text-align: center;
    color: #222222;
}
.other .item .icon{
    display: inline-block;
    width: 56rpx;
    height: 56rpx;
    margin-bottom: 10rpx;
}


.myBottom{
    padding-top: 140rpx;
    text-align: center;
    font-size: 28rpx;
    color: #CCCCCC;
}
.myBottom .title{
    font-weight: bold;
    margin-bottom: 16rpx;
}
