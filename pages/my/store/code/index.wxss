.container{
    padding: 250rpx 26rpx 0 26rpx;
    height: 100vh;
    background: #05C160;
}
.box{
    position: relative;
    width: 698rpx;
    /*height: 774rpx;*/
    padding: 0 53rpx;
    text-align: center;
    border-radius: 24rpx;
    background: #FFFFFF;
}
.title{
    font-size: 40rpx;
    text-align: center;
    font-weight: bold;
    color: #222222;
    padding: 60rpx 0;
}
.code{
    width: 440rpx;
    height: 440rpx;
    margin: auto;
    display: block;
}
.footer{
    font-size: 28rpx;
    text-align: center;
    font-weight: bold;
    /* 辅助色/绿 */
    color: #4DC185;
    line-height: 115rpx;
    border-top: 2rpx dashed #D8D8D8;
    margin-top: 58rpx;
}
.after{
    position: absolute;
    bottom: 94rpx;
    left: -20rpx;
    width: 42rpx;
    height: 42rpx;
    background: #05C160;
    border-radius: 50%;
}
.before{
    position: absolute;
    bottom: 94rpx;
    right: -20rpx;
    width: 42rpx;
    height: 42rpx;
    background: #05C160;
    border-radius: 50%;

}

.btn{
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 32rpx;
    color: #FFFFFF;
    margin-top: 98rpx;
}
.btn .icon{
    display: block;
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
}
