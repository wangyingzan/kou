/* 富文本 */
rich-text{
    width: 100%;
    margin: 16rpx auto;
    position: relative;
    display: block;
    word-break: break-all;
    white-space: pre-warp;
}
rich-text image,rich-text img{
    width: 100% !important;
}
rich-text table{
    width: 100% !important;
}
rich-text .span_class{

    width: 100%;
    font-size: 14px;
    line-height: 20px;

}

rich-text .p_class{
    width: 100%;
    font-size: 14px;
    line-height: 20px;
}

rich-text .img_class{
    display: block;
    width: 100% !important;
    height: auto !important;
}
