page{
    background: #f5f5f5;
}
.container{
    background: #f5f5f5;
}
.header{
    position: relative;
    margin-bottom: 24rpx;

}
.header .navTitle{
    position: relative;
    z-index: 10;
}
.header .navTitle .title{
    font-size: 36rpx;
    text-align: center;
    font-weight: bold;
    color: #222222;
}
.header .headerBg{
    position: absolute;
    top: 0;
    width: 100%;
    height: 406rpx;
}

.searchBox{
    position: relative;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 26rpx;
    z-index: 100;
    margin-top: 20rpx;
}
.searchBox .search{
    width: 630rpx;
    height: 80rpx;
    padding: 0 23rpx;
    display: flex;
    color: #999999;
    align-items: center;
    border-radius: 12rpx;
    background: rgba(255, 255, 255, 0.6);
    box-sizing: border-box;
    border: 2rpx solid rgba(255, 255, 255, 0.6);
}

.searchBox .search .iconSearch{
    width: 28rpx;
    height: 28rpx;
    margin-right: 10rpx;
}



.searchBox .scan{
    width: 43rpx;
    height: 43rpx;
}

.goodsLogBox{
    position: absolute;
    left: 24rpx;
    bottom: 53rpx;
    width: 286rpx;
    height: 46rpx;
    border-radius: 12rpx;
    padding: 6rpx 14rpx;
    background: rgba(0, 0, 0, 0.5);
    z-index: 1000;
}
.goodsLogBox .logBox{
    height: 46rpx;
    width: 100%;
    color: #fff;
}
.goodsLogBox .logBox .item{
    height: 46rpx;
    font-size: 24rpx;
    white-space: nowrap;
}
.bannerBox{
    position: relative;
}

.swiperBox{
    height: 315rpx;
}
.swiperBox .banner{
    display: block;
    height: 315rpx;
    width: 698rpx;
    border-radius: 20rpx;
}


.informBox{
    width: 698rpx;
    height: 80rpx;
    display: flex;
    flex-direction: row;
    font-family: PingFang SC;
    align-items: center;
    line-height: 80rpx;
    font-size: 24rpx;
    color: #023B1D;
    padding: 0 26rpx;
    border-radius: 12rpx;
    margin: 0 26rpx;
    margin-top: 26rpx;
    background: rgba(213, 233, 223, 0.4);
}
.informBox .item{
    flex: 1;
    height: 80rpx;
}
.informBox .icon{
    width: 30rpx;
    height: 30rpx;
    margin-right: 20rpx;

}
.informBox text{

}


.tabs{
    display: flex;
    align-items: center;
    font-family: PingFang SC;
    margin-top: 46rpx;
    font-weight: bold;
    margin-bottom: 40rpx;
}
.tabs .tab{
    position: relative;
    line-height: 44rpx;
    margin-right: 32rpx;
    font-size: 32rpx;
    color: #333333;
}
.tabs .tab.active{
    background: linear-gradient(108deg, #E65727 16%, #E69027 86%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    text-fill-color: transparent;
}
.tabs .tab .icon{
    display: none;
    position: absolute;
    left: 50%;
    bottom: -8rpx;
    width: 44rpx;
    height: 8rpx;
    transform: translate(-50%, 0);
}
.tabs .tab.active .icon{
    display: block;
}

.modal{
    padding:  130rpx 80rpx;
    background: rgba(0,0,0,0);
}

.noticeBox{
    position: relative;
    width: 576rpx;
    text-align: center;
    padding: 40rpx 52rpx;
    border-radius: 24rpx;
    background: linear-gradient(0deg, #DFF7EB 0%, #E6F5ED 100%);
    box-sizing: border-box;
    border: 2rpx solid ;
    backdrop-filter: blur(142rpx);
}
.noticeBox .title{
    font-size: 40rpx;
    color: #333333;
    font-weight: bold;
    margin-bottom: 30rpx;
}
.noticeBox .notice{
    text-align: left;
    min-height: 202rpx;
    color: #333333;
    font-size: 28rpx;
    margin-bottom: 12rpx;
}
.noticeBox .noticeBtn{
    width: 308rpx;
    line-height: 76rpx;
    font-size: 32rpx;
    color: #FFFFFF;
    text-align: center;
    border-radius: 38rpx;
    background: #05C160;
    margin: auto;
}
.noticeBox .closeBtn{
    position: absolute;
    left: 50%;
    bottom: -100rpx;
    width: 80rpx;
    height: 80rpx;
    transform: translate(-50%,0);
}
.noticeBox .icon{
    position: absolute;
    top: -110rpx;
    left: -84rpx;
    width: 230rpx;
    height: 230rpx;
}

.list{
    display: flex;
    flex-direction: row;
    gap:20rpx 18rpx;
    flex-wrap: wrap;
    padding-bottom: 20rpx;
}


.service{
    position: fixed;
    right: 30rpx;
    bottom: 254rpx;
    width: 80rpx;
    height: 80rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 50%;
    padding:0;
    background: rgba(0, 0, 0, 0.6);
}

.service image{
    width: 45rpx;
    height: 45rpx;
}
